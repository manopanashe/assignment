  <body>
    <%- include('common/header'); -%>
<div class="container">
 
  
    <div class="row col-md-12 col-md-offset-2 custyle mx-auto">
      

    <h1 class="mx-auto"> J Clark Stores  </h1>
    
    <table class="table table-striped custab">
    <thead>
        <tr>
            <th>Store Number</th>
            <th>Size</th>
           

        </tr>
      </thead>
        <%  stores.forEach(function(store)  {
            
        %>

            <tr>
                <td><%=  store.store_number  %></td>
                <td><%=  store.size  %></td>
                <td><%= store.type %></td>
               
                
                <td class="text-center"><a  href="#"class='btn btn-info btn-xs'><span class="glyphicon glyphicon-edit"></span> Edit</a> 
                  <a  href="#" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> Del</a></td>
                 
            </tr>
            
         <%  
          });
         %>


       
  
          
    </table>

     
    </div>
    
  
    <%- include('common/footer') -%>
  </body>
